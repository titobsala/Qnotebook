{
  "name": "qnotebook",
  "productName": "qnotebook",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "electron/main.js",
  "homepage": "./",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "electron-forge start -- --no-sandbox",
    "electron-dev": "NODE_ENV=development concurrently \"npm run dev\" \"wait-on http://localhost:5174 && npm run electron\"",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": "tito-sala",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "asar": true
      },
      "electronRebuildConfig": {},
      "electronWinstallerConfig": {
        "name": "qnotebook"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "qnotebook"
      }
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "quill": "^1.3.7"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "8.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^5.0.0",
    "wait-on": "^7.2.0"
  }
}
